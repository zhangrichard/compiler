antlr3a=`pwd`
export CLASSPATH=.:$antlr3a/stringtemplate4.jar:$antlr3a/antlr-3.5.jar
if test -z "$1"
then
  echo CS USAGE: antlr3 t.g, then antlr3 *.java, then antlr3 Main
  java org.antlr.Tool "$@"
else case "$1" in
  *.java)
    javac "$@"
    ;;
  *.g)
    java org.antlr.Tool "$@"
    ;;
  *)
    java "$@"
    ;;
esac fi
